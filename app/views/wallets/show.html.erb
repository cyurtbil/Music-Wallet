
	<h1><%= @wallet.name %></h1>
	<% @songs.each do |song| %>
		<% if song.wallet_id == @wallet.id %>
			<iframe width="100%" height="400" scrolling="no" frameborder="no" src="https://w.soundcloud.	com/player/?visual=true&url=https%3A%2F%2Fapi.soundcloud.com%2Ftracks%2F<%= song.track_id %>&show_artwork=tru&client_id=3e664ddb00dc36cccd7244884f13749d"></iframe>
			<br>
		<% end %>
	<% end %>
